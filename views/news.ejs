<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>News</title>
    </head>
    <body>
        <h1>Top News</h1>
        <ul>
            <% articles.forEach(article => { %>
            <li>
                <strong><%= article.title %></strong> -
                <a href="<%= article.url %>" target="_blank">Read more</a>
            </li>
            <% }) %>
        </ul>
        <a href="/">Go Back</a>
    </body>
</html>
